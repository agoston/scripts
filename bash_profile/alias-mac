alias top='top -o -cpu -s 2'
alias v='cd /Volumes'
alias xpoo='rm -rf .Spotlight-V100/ .Trashes/ ._.Trashes .fseventsd/'
# alias-mac executes after alias-home, so this alias is overwritten
alias s="cd /System/Volumes/Data/mount/seagoat/"

pqiv() {
	pqiv --command-2='exiftran -i -2' --command-9='exiftran -i -9' --command-4="cp -v '\$1' '${HOME}'" --command-5="cd '${HOME}'; rm -fv '\$1'" img_20191026_101722.jpg
}
